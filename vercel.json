{
    "version": 2,
    "builds": [
        {
            "src": "wsgi.py",
            "use": "@vercel/python",
            "env": {
                "TMPDIR": ".tmp"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ],
    "env": {
        "FLASK_APP": "wsgi.py",
        "FLASK_ENV": "production"
    }
}